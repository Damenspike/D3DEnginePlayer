  "license": "MIT",
  "scripts": {
    "editor": "node scripts/dev-editor.mjs",
    "player": "node scripts/dev-player.mjs",
    "bump:player": "node scripts/bump-version.mjs player",
    "bump:editor": "node scripts/bump-version.mjs editor",
    "build:editor": "yarn bump:editor && vite build --config vite.editor.config.js",
    "build:player": "yarn bump:player && vite build --config vite.player.config.js",
    "build": "yarn build:editor && yarn build:player",
    "pkg": "yarn pkg:player && yarn pkg:editor",
    "pkg:player": "sh scripts/player-build.sh",
    "pkg:editor": "sh scripts/editor-build.sh",
    "pkg:player:mac": "sh scripts/player-build-mac.sh",
    "pkg:editor:mac": "sh scripts/editor-build-mac.sh"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.1",
    "electron": "^37.3.1",
    "vite": "^7.1.3",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.3",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.4",
    "@dimforge/rapier3d-compat": "^0.19.0",
    "@monaco-editor/react": "^4.7.0",
    "@uiw/react-codemirror": "^4.25.2",
    "@vscode/codicons": "^0.0.41",
    "axios": "^1.11.0",
