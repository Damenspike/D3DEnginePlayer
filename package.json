    "jszip": "^3.10.1",
    "material-icons": "^1.13.14",
    "monaco-editor": "^0.53.0",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-best-gradient-color-picker": "^3.0.14",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "three": "^0.180.0",
    "uuid": "^11.1.0"
  }
